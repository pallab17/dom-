// when i am manipulating the h1 tag using inner html and using a em tag the em tag should be written inside the double 
// inverted commas


document.querySelector("h1").innerHTML;
' <strong>Hello</strong> '
document.querySelector("h1").textContent;
' Hello '
document.querySelector("h1").innerHTML="<em>pb7</em>";
'<em>pb7</em>'
document.querySelector("h1").innerHTML="pb7";
'pb7'
